<h1>Reservable Events</h1>

<div class="wrapper">
    <% @events.each do |event| %>
        <div class="event-card">
            <p><%= event.title %></p>
            <p>Hosted By: <%= event.user.full_name %></p>
            <p>Starts On <%= date_of event %> at <%= time_of event %></p>
        </div>
    <% end %>
</div>